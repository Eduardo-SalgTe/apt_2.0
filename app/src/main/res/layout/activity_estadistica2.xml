<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".estadistica2">

    <TextView
        android:id="@+id/tv_est2_titulo"
        android:layout_width="207dp"
        android:layout_height="35dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="4dp"
        android:text="@string/rbt_4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_est2_datos"
        android:layout_width="351dp"
        android:layout_height="130dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="20dp"
        android:text="@string/prueba"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_est2_titulo" />

    <TextView
        android:id="@+id/tv_est2_pagos"
        android:layout_width="333dp"
        android:layout_height="222dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="24dp"
        android:text="@string/prueba"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_est2_datos" />
</androidx.constraintlayout.widget.ConstraintLayout>